<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordovacn.www" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>CordovaCn</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="46517115@qq.com" href="https://github.com/ryouaki">
        CordovaCn Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
